a = int(input('Enter the first number: '))
b = int(input('Enter the second number: '))
c = int(input('Ener the third number: '))

D = b**2 - 4*a*c

if D > 0:
    x1 = (-b + (D ** 0.5)) / (2 * a)
    x2 = (-b - (D ** 0.5)) / (2 * a)
    print(f'The result for x1 is {x1:.2f}')
    print(f'The result for x2 is {x2:.2f}')
elif D == 0:
    x1 = x2 = -b / (2 * a)
    print(f'The result for x1 and x2 is:  {x1:.2f}')
else:
    print("No solutions")
